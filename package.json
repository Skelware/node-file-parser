{
  "name": "node-file-parser",
  "version": "2.2.2",
  "description": "A simple yet powerful module to parse any file.",
  "main": "dist/NodeFileParser.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Skelware/node-file-parser"
  },
  "bugs": {
    "url": "https://github.com/Skelware/node-file-parser/issues"
  },
  "homepage": "https://github.com/Skelware/node-file-parser",
  "scripts": {
    "prepublish": "npm run build",
    "postinstall": "npm run build",
    "build": "babel src --out-dir dist",
    "test": "jest --verbose",
    "dev": "jest --watch"
  },
  "keywords": [
    "ini",
    "srt",
    "json",
    "parse",
    "skelware",
    "asset_compress"
  ],
  "author": {
    "name": "Skelware",
    "email": "admin@skelware.com"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-jest": "^9.0.3",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "jest-cli": "^0.9.2"
  },
  "jest": {
    "collectCoverage": true,
    "testDirectoryName": "spec",
    "testFileExtensions": [
      "es6"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  },
  "babel": {
    "comments": false,
    "sourceMaps": "inline",
    "presets": [
      "es2015",
      "stage-0"
    ]
  }
